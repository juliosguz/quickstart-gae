{
  "name": "quickstart-gae",
  "version": "1.0.0",
  "description": "Quick start with Google App Engine",
  "scripts": {
    "deploy-app": "appcfg.py -A YOUR_PROJECT_ID -V v1 update .",
    "deploy-modules": "appcfg.py -A PROJECT_ID update_dispatch .",
    "appengine": "dev_appserver.py app.yaml --port=8181 ",
    "initial-layout": "gulp server",
    "start": "concurrently --kill-others \"npm run appengine\" \"npm run initial-layout\" "
  },
  "author": "Julio Silva <julio.sguz@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "browser-sync": "^2.11.1",
    "concurrently": "^2.0.0",
    "del": "^2.2.0",
    "gulp": "^3.9.0",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-changed": "^1.3.0",
    "gulp-concat": "^2.6.0",
    "gulp-csso": "^1.1.0",
    "gulp-if": "^2.0.0",
    "gulp-jshint": "^2.0.0",
    "gulp-load-plugins": "^1.2.0",
    "gulp-sass": "^2.2.0",
    "gulp-size": "^2.0.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-stylus": "^2.3.0",
    "gulp-uglify": "^1.5.3",
    "jshint-stylish": "^2.1.0",
    "opn": "^4.0.0",
    "require-dir": "^0.3.0",
    "run-sequence": "^1.1.5",
    "webpack-stream": "^3.1.0"
  }
}
